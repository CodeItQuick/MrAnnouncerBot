<UserControl x:Class="DHDM.User_Controls.ModsList"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DHDM.User_Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="AliceBlue">
    <StackPanel Orientation="Horizontal">
		<local:EditableListBox Title="Mods:" DataFileName="AllMods.json"></local:EditableListBox>
		<local:ModBuilder Margin="8" ></local:ModBuilder>     
    </StackPanel>
</UserControl>
