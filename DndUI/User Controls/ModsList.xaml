<UserControl x:Class="DndUI.ModsList"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
						 xmlns:ui="clr-namespace:DndUI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="AliceBlue">
    <StackPanel Orientation="Horizontal">
		<ui:EditableListBox Title="Mods:" DataFileName="AllMods.json" ItemDeleted="EditableListBox_ItemDeleted"></ui:EditableListBox>
		<ui:ModBuilder Margin="8"></ui:ModBuilder>     
    </StackPanel>
</UserControl>
